on run argv	set theUser to item 1 of argv	set thePassword to item 2 of argv	set theAdminUser to item 3 of argv	set theAdminPassword to item 4 of argv		set User to do shell script "/usr/bin/id -u " & theUser	do shell script "/System/Library/CoreServices/Menu\\ Extras/User.menu/Contents/Resources/CGSession -switchToUserID " & User user name theAdminUser password theAdminPassword with administrator privileges	tell application "System Events"		tell application process "loginwindow"			delay 3.0						keystroke thePassword						keystroke tab						keystroke return		end tell	end tellend run