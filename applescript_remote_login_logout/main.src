on run argv	set theUser to item 1 of argv	set thePassword to item 2 of argv	set theAdminUser to item 3 of argv	set theAdminPassword to item 4 of argv		tell application "Terminal"		activate		do script "osascript /var/tmp/wrapper.scpt " & theUser & " " & thePassword & " " & theAdminUser & " " & theAdminPassword	end tellend run